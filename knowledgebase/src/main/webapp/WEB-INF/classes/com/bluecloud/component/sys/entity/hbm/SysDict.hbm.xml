<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bluecloud.component.sys.entity.po.SysDict" table="sys_dict" dynamic-insert="true" dynamic-update="true" select-before-update="true">
        <id name="dictid" column="dictid" type="java.lang.Long">
            <generator class="native"/>
        </id>
        
        <property name="dictcode" column="dictcode" type="java.lang.String"/>
        
        <property name="parentcode" column="parentcode" type="java.lang.String"/>
        
        <property name="dictkey" column="dictkey" type="java.lang.String"/>
           
        <property name="dictname" column="dictname" type="java.lang.String"/>
           
        <property name="descinfo" column="descinfo" type="java.lang.String"/>
        
        <property name="creater" column="creater" type="java.lang.String"/>
        
        <property name="createtime" column="createtime" type="java.lang.String"/>
        
        <property name="editer" column="editer" type="java.lang.String"/>
        
        <property name="edittime" column="edittime" type="java.lang.String"/>
        
        <property name="enabled" column="enabled" type="java.lang.Long"/>
            
    </class>
</hibernate-mapping>
