<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bluecloud.component.sys.entity.po.SysUser" table="sys_user" dynamic-update="false">
        <id name="userid" column="userid" type="java.lang.Long">
            <generator class="native"/>
        </id>
        <property name="loginuser" column="loginuser" type="java.lang.String"/>
        <property name="loginpwd" column="loginpwd" type="java.lang.String"/>      
        <property name="usercode" column="usercode" type="java.lang.String"/>      
        <property name="username" column="username" type="java.lang.String"/>
        <property name="usernick" column="usernick" type="java.lang.String"/>
        <property name="orgid" column="orgid" type="java.lang.Long"/>
        <property name="usersex" column="usersex" type="java.lang.Long"/>            
        <property name="userjob" column="userjob" type="java.lang.String"/>            
       

        <property name="usertel" column="usertel" type="java.lang.String"/>       
        <property name="usermp" column="usermp" type="java.lang.String"/>            
        <property name="useremail" column="useremail" type="java.lang.String"/>            
        <property name="userqq"  column="userqq" type="java.lang.String"/>           
        <property name="usermsn" column="usermsn" type="java.lang.String"/>
        <property name="userimg" column="userimg" type="java.lang.String"/>
        <property name="userboth" column="userboth" type="java.lang.String"/> 
        <property name="userfax" column="userfax" type="java.lang.String"/> 
        <property name="usercity" column="usercity" type="java.lang.String"/> 
        <property name="userdesc" column="userdesc" type="java.lang.String"/> 
        <property name="userlevel" column="userlevel" type="java.lang.String"/>             
   
        <property name="creater" column="creater" type="java.lang.String"/>
        <property name="createtime" column="createtime" type="java.lang.String"/>        
        <property name="editer" column="editer" type="java.lang.String"/>		
		<property name="edittime" column="edittime" type="java.lang.String"/>
		<property name="enabled" column="enabled" type="java.lang.Long"/>
		
		<property name="usertype" column="usertype" type="java.lang.String"/>
    </class>
</hibernate-mapping>
