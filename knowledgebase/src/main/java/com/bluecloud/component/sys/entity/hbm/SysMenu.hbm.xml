<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bluecloud.component.sys.entity.po.SysMenu" table="sys_menu">
        <id name="menucode" column="menucode" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property name="menuname" column="menuname" type="java.lang.String"/>
           
        <property name="menuenname" column="menuenname" type="java.lang.String"/>
           
        <property name="parentcode" column="parentcode" type="java.lang.String"/>
           
        <property name="menuurl" column="menuurl" type="java.lang.String" />
           
        <property name="menuflag" column="menuflag" type="java.lang.String"/>
        <property name="menuicon" column="menuicon" type="java.lang.String" />
            
        <property name="sequ" column="sequ" type="java.lang.Long"/>
        <property name="enabled" column="enabled" type="java.lang.Long"/>
        
        <property name="syscode" column="syscode" type="java.lang.String"/>
        

    </class>
</hibernate-mapping>
